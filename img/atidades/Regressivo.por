programa {
  funcao inicio() {

    inteiro numero, valor, cont

    escreva("Escreva um numero:\n")
    leia(numero)

   se(numero > 0){
    escreva("Contagem regressiva:")
    para(cont = numero; cont >= 0; cont--){
      escreva(cont,"\n")
    }
    escreva("Fianlizado")
   }
      
    }
    
  }

